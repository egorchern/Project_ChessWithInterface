<Window x:Name="ParametersWindow" x:Class="Project_ChessWithInterface.StartGameParameters"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_ChessWithInterface"
        mc:Ignorable="d"
        Title="GameStartParametersWindow" Height="670" Width="758" ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="310" Margin="14,66,0,0" VerticalAlignment="Top" Width="287"/>
        <Label x:Name="label" Content="Who do you want to play against?" HorizontalAlignment="Left" Margin="30,24,0,0" VerticalAlignment="Top" FontWeight ="Bold" FontSize="15"/>
        <Button x:Name="submit_btn" Content="Start" HorizontalAlignment="Left" Margin="319,515,0,0" VerticalAlignment="Top" Width="121" FontWeight="Bold" FontSize="30" Height="66"/>

    </Grid>
</Window>
